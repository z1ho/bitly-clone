<center>
	<div id="tableinfo">
    <table style="text-align: center;">
     <tr>
         <th id="longurl"><u>Long URL</u></th>
         <th style="padding: 0px 50px 0px 50px"><u>Short URL</u></th>
         <th id="numclicks"><u>Number of Clicks</u></th>
     </tr>

      <tr>
      <% @url.each do |elem| %>
         <td> <a href = "/<%= elem.long_url %>"><%= elem.long_url %> </a></td>
         <td> <id="shorturl" a href= "/<%= elem.short_url %>"><%= elem.short_url %> </a></td>
         <td> <%= elem.click_count %> </td>
      </tr>
 <% end %>
    </table>
  </div>
</center>

